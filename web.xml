<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>gameData</servlet-name>
        <servlet-class>com.google.sps.servlets.getGamesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gameData</servlet-name>
        <url-pattern>/api/gameData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>articleData</servlet-name>
        <servlet-class>com.google.sps.servlets.getArticlesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>articleData</servlet-name>
        <url-pattern>/api/articleData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>videoData</servlet-name>
        <servlet-class>com.google.sps.servlets.getVideosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>videoData</servlet-name>
        <url-pattern>/api/videoData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>create-event</servlet-name>
        <servlet-class>com.google.sps.servlets.CreateEventServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>create-event</servlet-name>
        <url-pattern>/api/create-event</url-pattern>
    </servlet-mapping>
</web-app>